<Window x:Class="LaserTestingApp.ImportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LaserTestingApp"
        mc:Ignorable="d"
        Title="ImportWindow" Height="250" Width="400">
    <Grid>
        <Button x:Name="ImportButton" Content="Import" HorizontalAlignment="Center" Margin="0,0,0,0" VerticalAlignment="Center" Width="74" Click="ImportButton_Click" FontWeight="Bold" FontSize="18"/>
        <ProgressBar HorizontalAlignment="Center" Height="20" Margin="0,154,0,0" VerticalAlignment="Top" Width="138"/>
    </Grid>
</Window>
