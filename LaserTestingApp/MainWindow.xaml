<Window x:Class="LaserTestingApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LaserTestingApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="750" Width="1300">
    <Grid>
        <Button x:Name ="LoadDataButton" Content="Load Data" HorizontalAlignment="Left" Margin="128,646,0,0" VerticalAlignment="Top" Click="LoadDataButton_Click"/>
        <DataGrid x:Name ="LaserDataGrid" ItemsSource="{Binding}" AutoGenerateColumns="False" Margin="10,10,842,331">
        <DataGrid.Columns>
        <DataGridTextColumn Header="Time"  Binding="{Binding Time}"/>
        <DataGridTextColumn Header="Ambient Temp" Binding="{Binding AmbientTemp}" />
        <DataGridTextColumn Header="Unit Temp" Binding="{Binding UnitTemp}" />
        <DataGridTextColumn Header="Divergence" Binding="{Binding Divergence}" />
        <DataGridTextColumn Header="Power Output" Binding="{Binding PowerOutput}" />
        </DataGrid.Columns>
        </DataGrid>
        <Rectangle HorizontalAlignment="Left" Height="563" Margin="560,55,0,0" Stroke="Black" VerticalAlignment="Top" Width="608"/>
    </Grid>
</Window>
